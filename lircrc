    begin
        prog   = irexec
        button = KEY_VOLUMEUP
        config = python /root/pi3/up.py
        repeat = 1
    end

    begin
        prog   = irexec
        button = KEY_VOLUMEDOWN
        config = python /root/pi3/down.py
        repeat = 1
    end

